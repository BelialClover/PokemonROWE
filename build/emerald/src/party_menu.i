void ItemUseCB_Medicine(u8 taskId, TaskFunc task)
{
    u16 hp = 0;
    struct Pokemon *mon = &gPlayerParty[gPartyMenu.slotId];
    u16 item = gSpecialVar_ItemId;
    bool8 canHeal, cannotUse;

    if (NotUsingHPEVItemOnShedinja(mon, item) == 0)
    {
        cannotUse = 1;
    }
    else
    {
        canHeal = IsHPRecoveryItem(item);
        if (canHeal == 1)
        {
            hp = GetMonData(mon, 57);
            if (hp == GetMonData(mon, 58))
                canHeal = 0;
        }
        cannotUse = ExecuteTableBasedItemEffect_(gPartyMenu.slotId, item, 0);
    }

    if (cannotUse != 0)
    {
        gPartyMenuUseExitCallback = 0;
        PlaySE(5);
        DisplayPartyMenuMessage(gText_WontHaveEffect, 1);
        ScheduleBgCopyTilemapToVram(2);
        if (gPartyMenu.menuType == 0 && CheckBagHasItem(item, 1))
            gTasks[taskId].func = Task_ReturnToChooseMonAfterText;
        else
            gTasks[taskId].func = task;
    }
    else
    {
        gPartyMenuUseExitCallback = 1;
        if (!IsItemFlute(item))
        {
            PlaySE(1);
            if (gPartyMenu.action != 14)
                RemoveBagItem(item, 1);
        }
        else
        {
            PlaySE(117);
        }
        SetPartyMonAilmentGfx(mon, &sPartyMenuBoxes[gPartyMenu.slotId]);
        if (gSprites[sPartyMenuBoxes[gPartyMenu.slotId].statusSpriteId].invisible)
            DisplayPartyPokemonLevelCheck(mon, &sPartyMenuBoxes[gPartyMenu.slotId], 1);
        if (canHeal == 1)
        {
            if (hp == 0)
                AnimatePartySlot(gPartyMenu.slotId, 1);
            PartyMenuModifyHP(taskId, gPartyMenu.slotId, 1, GetMonData(mon, 57) - hp, Task_DisplayHPRestoredMessage);
            ResetHPTaskData(taskId, 0, hp);
            return;
        }
        else
        {
            GetMonNickname(mon, gStringVar1);
            GetMedicineItemEffectMessage(item);
            DisplayPartyMenuMessage(gStringVar4, 1);
            ScheduleBgCopyTilemapToVram(2);
            if (canHeal == 1)
            {
                if (hp == 0)
                    AnimatePartySlot(gPartyMenu.slotId, 1);
                PartyMenuModifyHP(taskId, gPartyMenu.slotId, 1, GetMonData(mon, 57) - hp, Task_DisplayHPRestoredMessage);
                ResetHPTaskData(taskId, 0, hp);
                return;
            }
            else
            {
                GetMonNickname(mon, gStringVar1);
                GetMedicineItemEffectMessage(item);
                DisplayPartyMenuMessage(gStringVar4, 1);
                ScheduleBgCopyTilemapToVram(2);
                if (gPartyMenu.menuType == 0 && CheckBagHasItem(item, 1))
                    gTasks[taskId].func = Task_ReturnToChooseMonAfterText;
                else
                    gTasks[taskId].func = task;
            }
        }
    }
}
