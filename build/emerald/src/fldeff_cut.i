bool8 SetUpFieldMove_Cut(void)
{
    s16 x, y;
    u8 i, j;
    u8 tileBehavior;
    u16 userAbility;
    bool8 cutTiles[3 * 3];
    bool8 ret;

    if (CheckObjectGraphicsInFrontOfPlayer(82) == 1)
    {

        gFieldCallback2 = FieldCallback_PrepareFadeInFromMenu;
        gPostMenuFieldCallback = FieldCallback_CutTree;
        return 1;
    }
    else
    {
        PlayerGetDestCoords(&gPlayerFacingPosition.x, &gPlayerFacingPosition.y);
        userAbility = GetMonAbility(&gPlayerParty[GetCursorSelectionMonId()]);
        if (userAbility == 52)
        {
            sCutSquareSide = 5;
            sTileCountFromPlayer_X = 2;
            sTileCountFromPlayer_Y = 2;
        }
        else
        {
            sCutSquareSide = 3;
            sTileCountFromPlayer_X = 1;
            sTileCountFromPlayer_Y = 1;
        }

        for (i = 0; i < 3 * 3; i++)
            cutTiles[i] = 0;
        for (i = 0; i < 5 * 5; i++)
            sHyperCutTiles[i] = 0;

        ret = 0;

        for (i = 0; i < 3; i++)
        {
            y = i - 1 + gPlayerFacingPosition.y;
            for (j = 0; j < 3; j++)
            {
                x = j - 1 + gPlayerFacingPosition.x;
                if (MapGridGetZCoordAt(x, y) == gPlayerFacingPosition.height)
                {
                    tileBehavior = MapGridGetMetatileBehaviorAt(x, y);
                    if (MetatileBehavior_IsPokeGrass(tileBehavior) == 1
                    || MetatileBehavior_IsAshGrass(tileBehavior) == 1)
                    {

                        sHyperCutTiles[6 + (i * 5) + j] = 1;
                        ret = 1;
                    }
                    if (MapGridIsImpassableAt(x, y) == 1)
                    {
                        cutTiles[i * 3 + j] = 0;
                    }
                    else
                    {
                        cutTiles[i * 3 + j] = 1;
                        if (MetatileBehavior_IsCuttableGrass(tileBehavior) == 1)
                            sHyperCutTiles[6 + (i * 5) + j] = 1;
                    }
                }
                else
                {
                    cutTiles[i * 3 + j] = 0;
                }
            }
        }

        if (userAbility != 52)
        {
            if (ret == 1)
            {
                gFieldCallback2 = FieldCallback_PrepareFadeInFromMenu;
                gPostMenuFieldCallback = FieldCallback_CutGrass;
            }
        }
        else
        {
            bool8 tileCuttable;
            for (i = 0; i < 16; i++)
            {
                x = gPlayerFacingPosition.x + sHyperCutStruct[i].x;
                y = gPlayerFacingPosition.y + sHyperCutStruct[i].y;
                tileCuttable = 1;

                j = 0;
                do
                {
                    if (sHyperCutStruct[i].unk2[j] == 0)
                        break;
                    if (cutTiles[(u8)(sHyperCutStruct[i].unk2[j] - 1)] == 0)
                    {
                        tileCuttable = 0;
                        break;
                    }
                } while (++j <= 1);

                if (tileCuttable == 1)
                {
                    if (MapGridGetZCoordAt(x, y) == gPlayerFacingPosition.height)
                    {
                        u8 tileArrayId = ((sHyperCutStruct[i].y * 5) + 12) + (sHyperCutStruct[i].x);
                        tileBehavior = MapGridGetMetatileBehaviorAt(x, y);
                        if (MetatileBehavior_IsPokeGrass(tileBehavior) == 1
                        || MetatileBehavior_IsAshGrass(tileBehavior) == 1)
                        {
                            gFieldCallback2 = FieldCallback_PrepareFadeInFromMenu;
                            gPostMenuFieldCallback = FieldCallback_CutGrass;
                            sHyperCutTiles[tileArrayId] = 1;
                            ret = 1;
                        }
                        else
                        {
                            if (MetatileBehavior_IsCuttableGrass(tileBehavior) == 1)
                                sHyperCutTiles[tileArrayId] = 1;
                        }
                    }
                }
            }

            if (ret == 1)
            {
                gFieldCallback2 = FieldCallback_PrepareFadeInFromMenu;
                gPostMenuFieldCallback = FieldCallback_CutGrass;
            }
        }

        return ret;
    }
}

