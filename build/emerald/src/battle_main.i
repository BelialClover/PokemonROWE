static void TryEvolvePokemon(void)
{
    s32 i;

    while (gLeveledUpInBattle != 0)
    {
        for (i = 0; i < 6; i++)
        {
            if (gLeveledUpInBattle & gBitTable[i])
            {
                u16 species;
                u8 formId;
                u8 levelUpBits = gLeveledUpInBattle;

                levelUpBits &= ~(gBitTable[i]);
                gLeveledUpInBattle = levelUpBits;
                species = GetEvolutionTargetSpecies(&gPlayerParty[i], 0, levelUpBits, 0, &formId);
                if (species != 0)
                {
                    FreeAllWindowBuffers();
                    gBattleMainFunc = WaitForEvoSceneToFinish;
                    EvolutionScene(&gPlayerParty[i], species, 1, i, formId);
                    return;
                }
            }
        }
    }

    gBattleMainFunc = ReturnFromBattleToOverworld;
}

static void WaitForEvoSceneToFinish(void)
{
    if (gMain.callback2 == BattleMainCB2)
        gBattleMainFunc = TryEvolvePokemon;
}

static void ReturnFromBattleToOverworld(void)
{
    if (!(gBattleTypeFlags & (1 << 1)))
    {
        RandomlyGivePartyPokerus(gPlayerParty);
        PartySpreadPokerus(gPlayerParty);
    }

    if (gBattleTypeFlags & (1 << 1) && gReceivedRemoteLinkPlayers != 0)
        return;

    gSpecialVar_Result = gBattleOutcome;
    gMain.inBattle = 0;
    gMain.callback1 = gPreBattleCallback1;

    if (gBattleTypeFlags & (1 << 10))
    {
        UpdateRoamerHPStatus(&gEnemyParty[0]);
        if ((gBattleOutcome & 1) || gBattleOutcome == 7)
            SetRoamerInactive();
    }

    m4aSongNumStop(90);
    SetMainCallback2(gMain.savedCallback);
}

void RunBattleScriptCommands_PopCallbacksStack(void)
{
    if (gCurrentActionFuncId == 11 || gCurrentActionFuncId == 12)
    {
        if (gBattleResources->battleCallbackStack->size != 0)
            gBattleResources->battleCallbackStack->size--;
        gBattleMainFunc = gBattleResources->battleCallbackStack->function[gBattleResources->battleCallbackStack->size];
    }
    else
    {
        if (gBattleControllerExecFlags == 0)
            gBattleScriptingCommandsTable[gBattlescriptCurrInstr[0]]();
    }
}

void RunBattleScriptCommands(void)
{
    if (gBattleControllerExecFlags == 0)
        gBattleScriptingCommandsTable[gBattlescriptCurrInstr[0]]();
}

void SetTypeBeforeUsingMove(u16 move, u8 battlerAtk)
{
    u32 moveType, ateType, attackerAbility;

    if (move == 165)
        return;

    gBattleStruct->dynamicMoveType = 0;
    gBattleStruct->ateBoost[battlerAtk] = 0;
    gSpecialStatuses[battlerAtk].gemBoost = 0;

    if (gBattleMoves[move].effect == 203)
    {
        if (((!IsAbilityOnField(13) && !IsAbilityOnField(76))))
        {
            if (gBattleWeather & ((1 << 0) | (1 << 1) | (1 << 2)))
                gBattleStruct->dynamicMoveType = 11 | 0x80;
            else if (gBattleWeather & ((1 << 3) | (1 << 4)))
                gBattleStruct->dynamicMoveType = 5 | 0x80;
            else if (gBattleWeather & ((1 << 5) | (1 << 6)))
                gBattleStruct->dynamicMoveType = 10 | 0x80;
            else if (gBattleWeather & ((1 << 7) | (1 << 8)))
                gBattleStruct->dynamicMoveType = 15 | 0x80;
            else
                gBattleStruct->dynamicMoveType = 0 | 0x80;
        }
    }
    else if (gBattleMoves[move].effect == 135)
    {
        u8 typeBits = ((gBattleMons[battlerAtk].hpIV & 1) << 0)
                     | ((gBattleMons[battlerAtk].attackIV & 1) << 1)
                     | ((gBattleMons[battlerAtk].defenseIV & 1) << 2)
                     | ((gBattleMons[battlerAtk].speedIV & 1) << 3)
                     | ((gBattleMons[battlerAtk].spAttackIV & 1) << 4)
                     | ((gBattleMons[battlerAtk].spDefenseIV & 1) << 5);

        gBattleStruct->dynamicMoveType = (15 * typeBits) / 63 + 1;
        if (gBattleStruct->dynamicMoveType >= 9)
            gBattleStruct->dynamicMoveType++;
        gBattleStruct->dynamicMoveType |= 0xC0;
    }
    else if (gBattleMoves[move].effect == 299)
    {
        if (GetBattlerHoldEffect(battlerAtk, 1) == gBattleMoves[move].argument)
            gBattleStruct->dynamicMoveType = ItemId_GetSecondaryId(gBattleMons[battlerAtk].item) | 0x80;
    }
    else if (gBattleMoves[move].effect == 316)
    {
        if (gBattleMons[battlerAtk].type1 != 9)
            gBattleStruct->dynamicMoveType = gBattleMons[battlerAtk].type1 | 0x80;
        else if (gBattleMons[battlerAtk].type2 != 9)
            gBattleStruct->dynamicMoveType = gBattleMons[battlerAtk].type2 | 0x80;
        else if (gBattleMons[battlerAtk].type3 != 9)
            gBattleStruct->dynamicMoveType = gBattleMons[battlerAtk].type3 | 0x80;
    }
    else if (gBattleMoves[move].effect == 216)
    {
        if (ItemId_GetPocket(gBattleMons[battlerAtk].item) == 7)
            gBattleStruct->dynamicMoveType = gNaturalGiftTable[(((gBattleMons[battlerAtk].item - 144) + 1))].type;
    }

    attackerAbility = GetBattlerAbility(battlerAtk);
    { if (gBattleStruct->dynamicMoveType) moveType = gBattleStruct->dynamicMoveType & 0x3F; else moveType = gBattleMoves[move].type; };
    if ((gFieldStatuses & 0x400 && moveType == 0)
        || gStatuses3[battlerAtk] & (1 << 30))
    {
        gBattleStruct->dynamicMoveType = 0x80 | 13;
    }
    else if (gBattleMoves[move].type == 0
             && gBattleMoves[move].effect != 135
             && gBattleMoves[move].effect != 203
             && gBattleMoves[move].effect != 299
             && gBattleMoves[move].effect != 216
             && ((attackerAbility == 182 && (ateType = 18))
                 || (attackerAbility == 174 && (ateType = 15))
                 || (attackerAbility == 184 && (ateType = 2))
                 || ((attackerAbility == 206) && (ateType = 13))
                )
             )
    {
        gBattleStruct->dynamicMoveType = 0x80 | ateType;
        gBattleStruct->ateBoost[battlerAtk] = 1;
    }
    else if (gBattleMoves[move].type != 0
             && gBattleMoves[move].effect != 135
             && gBattleMoves[move].effect != 203
             && attackerAbility == 96)
    {
        gBattleStruct->dynamicMoveType = 0x80 | 0;
        gBattleStruct->ateBoost[battlerAtk] = 1;
    }
    else if (gBattleMoves[move].flags & (1 << 16)
             && attackerAbility == 204)
    {
        gBattleStruct->dynamicMoveType = 0x80 | 11;
    }


    { if (gBattleStruct->dynamicMoveType) moveType = gBattleStruct->dynamicMoveType & 0x3F; else moveType = gBattleMoves[move].type; };
    if (GetBattlerHoldEffect(battlerAtk, 1) == 119
        && moveType == ItemId_GetSecondaryId(gBattleMons[battlerAtk].item))
    {
        gSpecialStatuses[battlerAtk].gemParam = GetBattlerHoldEffectParam(battlerAtk);
        gSpecialStatuses[battlerAtk].gemBoost = 1;
    }
}
