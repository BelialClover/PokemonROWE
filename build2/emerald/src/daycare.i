static u16 DetermineEggSpeciesAndParentSlots(struct DayCare *daycare, u8 *parentSlots)
{
    u16 i;
    u16 species[2];
    u16 eggSpecies;

    for (i = 0; i < 2; i++)
    {
        species[i] = GetBoxMonData(&daycare->mons[i].mon, 11);
        if (species[i] == 132)
        {
            parentSlots[0] = i ^ 1;
            parentSlots[1] = i;
        }
        else if (GetBoxMonGender(&daycare->mons[i].mon) == 0xFE)
        {
            parentSlots[0] = i;
            parentSlots[1] = i ^ 1;
        }
    }

    eggSpecies = GetEggSpecies(species[parentSlots[0]]);
    if (eggSpecies == 29 && daycare->offspringPersonality & 0x8000)
    {
        eggSpecies = 32;
    }
    if (eggSpecies == 314 && daycare->offspringPersonality & 0x8000)
    {
        eggSpecies = 313;
    }


    if (species[parentSlots[1]] == 132 && GetBoxMonGender(&daycare->mons[parentSlots[0]].mon) != 0xFE)
    {
        u8 ditto = parentSlots[1];
        parentSlots[1] = parentSlots[0];
        parentSlots[0] = ditto;
    }

    return eggSpecies;
}

