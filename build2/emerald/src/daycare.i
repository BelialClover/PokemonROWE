bool8 NameHasGenderSymbol(const u8 *name, u8 genderRatio)
{
    u8 i;
    u8 symbolsCount[2];
    symbolsCount[0] = symbolsCount[1] = 0;

    for (i = 0; name[i] != 0xFF; i++)
    {
        if (name[i] == 0xB5)
            symbolsCount[0]++;
        if (name[i] == 0xB6)
            symbolsCount[1]++;
    }

    if (genderRatio == 0x00 && symbolsCount[0] != 0 && symbolsCount[1] == 0)
        return 1;
    if (genderRatio == 0xFE && symbolsCount[1] != 0 && symbolsCount[0] == 0)
        return 1;

    return 0;
}

static u8 *AppendGenderSymbol(u8 *name, u8 gender)
{
    if (gender == 0x00)
    {
        if (!NameHasGenderSymbol(name, 0x00))
            return StringAppend(name, gText_MaleSymbol4);
    }
    else if (gender == 0xFE)
    {
        if (!NameHasGenderSymbol(name, 0xFE))
            return StringAppend(name, gText_FemaleSymbol4);
    }

    return StringAppend(name, gText_GenderlessSymbol);
}

