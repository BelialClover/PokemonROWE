static bool8 TryProduceOrHatchEgg(struct DayCare *daycare)
{
    u32 i, validEggs = 0;

    for (i = 0; i < 2; i++)
    {
        if (GetBoxMonData(&daycare->mons[i].mon, 5))
            daycare->mons[i].steps++, validEggs++;
    }


    if (daycare->offspringPersonality == 0 && validEggs == 2 && (daycare->mons[1].steps & 0xFF) == 0xFF)
    {
        u8 compatability = ModifyBreedingScoreForOvalCharm(GetDaycareCompatibilityScore(daycare));
        if (compatability > (Random() * 100u) / 65535)
            TriggerPendingDaycareEgg();
    }


    if (++daycare->stepCounter == 255)
    {
        u32 eggCycles;
        u8 toSub = GetEggCyclesToSubtract();

        for (i = 0; i < gPlayerPartyCount; i++)
        {
            if (!GetMonData(&gPlayerParty[i], 45))
                continue;
            if (GetMonData(&gPlayerParty[i], 4))
                continue;

            eggCycles = GetMonData(&gPlayerParty[i], 32);
            if (eggCycles != 0)
            {
                if (eggCycles >= toSub)
                    eggCycles -= toSub;
                else
                    eggCycles -= 1;

                SetMonData(&gPlayerParty[i], 32, &eggCycles);
            }
            else
            {
                gSpecialVar_0x8004 = i;
                return 1;
            }
        }
    }

    return 0;
}

