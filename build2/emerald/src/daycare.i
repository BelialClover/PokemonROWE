static u8 GetDaycareCompatibilityScore(struct DayCare *daycare)
{
    u32 i;
    u16 eggGroups[2][2];
    u16 species[2];
    u32 trainerIds[2];
    u32 genders[2];

    for (i = 0; i < 2; i++)
    {
        u32 personality;

        species[i] = GetBoxMonData(&daycare->mons[i].mon, 11);
        trainerIds[i] = GetBoxMonData(&daycare->mons[i].mon, 1);
        personality = GetBoxMonData(&daycare->mons[i].mon, 0);
        genders[i] = GetGenderFromSpeciesAndPersonality(species[i], personality);
        eggGroups[i][0] = gBaseStats[species[i]].eggGroup1;
        eggGroups[i][1] = gBaseStats[species[i]].eggGroup2;
    }


    if (eggGroups[0][0] == 15 || eggGroups[1][0] == 15)
        return 0;

    if (eggGroups[0][0] == 13 && eggGroups[1][0] == 13)
        return 0;


    if (eggGroups[0][0] == 13 || eggGroups[1][0] == 13)
    {
        if (trainerIds[0] == trainerIds[1])
            return 35;

        return 50;
    }

    else
    {
        if (genders[0] == genders[1])
            return 0;
        if (genders[0] == 0xFF || genders[1] == 0xFF)
            return 0;
        if (!EggGroupsOverlap(eggGroups[0], eggGroups[1]))
            return 0;

        if (species[0] == species[1])
        {
            if (trainerIds[0] == trainerIds[1])
                return 50;

            return 75;
        }
        else
        {
            if (trainerIds[0] != trainerIds[1])
                return 50;

            return 35;
        }
    }
}

