static void TaskFunc_UpdateWavePerFrame(u8 taskId)
{
    int value = 0;
    int i;
    int offset;

    if (sShouldStopWaveTask)
    {
        DestroyTask(taskId);
        gScanlineEffect.waveTaskId = 0xFF;
    }
    else
    {
        if (gTasks[taskId].data[7])
        {
            switch (gTasks[taskId].data[6])
            {
            case ((0x4000000 + 0x10) - (0x4000000 + 0x10)):
                value = gBattle_BG0_X;
                break;
            case ((0x4000000 + 0x12) - (0x4000000 + 0x10)):
                value = gBattle_BG0_Y;
                break;
            case ((0x4000000 + 0x14) - (0x4000000 + 0x10)):
                value = gBattle_BG1_X;
                break;
            case ((0x4000000 + 0x16) - (0x4000000 + 0x10)):
                value = gBattle_BG1_Y;
                break;
            case ((0x4000000 + 0x18) - (0x4000000 + 0x10)):
                value = gBattle_BG2_X;
                break;
            case ((0x4000000 + 0x1a) - (0x4000000 + 0x10)):
                value = gBattle_BG2_Y;
                break;
            case ((0x4000000 + 0x1c) - (0x4000000 + 0x10)):
                value = gBattle_BG3_X;
                break;
            case ((0x4000000 + 0x1e) - (0x4000000 + 0x10)):
                value = gBattle_BG3_Y;
                break;
            }
        }
        if (gTasks[taskId].data[4] != 0)
        {
            gTasks[taskId].data[4]--;
            offset = gTasks[taskId].data[3] + 320;
            for (i = gTasks[taskId].data[0]; i < gTasks[taskId].data[1]; i++)
            {
                gScanlineEffectRegBuffers[gScanlineEffect.srcBuffer][i] = gScanlineEffectRegBuffers[0][offset] + value;
                offset++;
            }
        }
        else
        {
            gTasks[taskId].data[4] = gTasks[taskId].data[5];
            offset = gTasks[taskId].data[3] + 320;
            for (i = gTasks[taskId].data[0]; i < gTasks[taskId].data[1]; i++)
            {
                gScanlineEffectRegBuffers[gScanlineEffect.srcBuffer][i] = gScanlineEffectRegBuffers[0][offset] + value;
                offset++;
