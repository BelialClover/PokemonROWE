static void GiveBattlePoints(void)
{
    s32 challengeNum = 0;
    s32 lvlMode = gSaveBlock2Ptr->frontier.lvlMode;
    s32 facility = VarGet(0x40CF);
    s32 battleMode = VarGet(0x40CE);
    s32 points;

