        }
        break;
    case 2:
        entryNum = b + 5;
        vOffset = sPokedexView->listVOffset + 10;
        if (vOffset >= 16)
            vOffset -= 16;
        if (entryNum < 0 || entryNum >= 898 || sPokedexView->pokedexList[entryNum].dexNum == 0xFFFF)
