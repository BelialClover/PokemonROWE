static inline void WritePixel(u8 *dst, u32 x, u32 y, u32 value)
{
    if (x & 1)
    {
        dst[( ((y / 8) * 32 * 8) + ((x / 8) * 32) + ((((y) - ((y / 8) * 8))) * 4) + ((((x) - ((x / 8) * 8)) / 2)))] &= ~0xF0;
        dst[( ((y / 8) * 32 * 8) + ((x / 8) * 32) + ((((y) - ((y / 8) * 8))) * 4) + ((((x) - ((x / 8) * 8)) / 2)))] |= (value << 4);
    }
    else
    {
        dst[( ((y / 8) * 32 * 8) + ((x / 8) * 32) + ((((y) - ((y / 8) * 8))) * 4) + ((((x) - ((x / 8) * 8)) / 2)))] &= ~0xF;
