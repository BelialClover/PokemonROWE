                    name = sMultiNameFlyDestinations[i].name[sFlyMap->regionMap.posWithinMapSec];
                    AddTextPrinterParameterized(1, 1, name, GetStringRightAlignXOffset(1, name, 96), 17, 0, ((void *)0));
                    ScheduleBgCopyTilemapToVram(0);
                    gUnknown_03001180 = 1;
                }
                break;
            }
        }
        if (!namePrinted)
        {
            if (gUnknown_03001180 == 1)
            {
                ClearStdWindowAndFrameToTransparent(1, 0);
                DrawStdFrameWithCustomTileAndPalette(0, 0, 101, 13);
