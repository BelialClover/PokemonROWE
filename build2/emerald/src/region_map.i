    if (sFlyMap->regionMap.mapSecType > MAPSECTYPE_NONE && sFlyMap->regionMap.mapSecType <= MAPSECTYPE_BATTLE_FRONTIER)
    {
        namePrinted = 0;
        for (i = 0; i < (size_t)(sizeof(sMultiNameFlyDestinations) / sizeof((sMultiNameFlyDestinations)[0])); i++)
