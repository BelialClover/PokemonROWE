    return sTerraOrMarineCaveMapSecIds[idx];
}

static void GetMarineCaveCoords(u16 *x, u16 *y)
{
    u16 idx;

    idx = VarGet(0x4037);
    if (idx < (1 + 8) || idx > (8 + 8))
    {
        idx = (1 + 8);
    }
    idx -= (1 + 8);

    *x = sMarineCaveLocationCoords[idx].x + 1;
    *y = sMarineCaveLocationCoords[idx].y + 2;
}



