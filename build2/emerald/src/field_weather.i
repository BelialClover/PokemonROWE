void FadeScreen(u8 mode, s8 delay)
{
    u32 fadeColor;
    bool8 fadeOut;
    bool8 useWeatherPal;

    switch (mode)
    {
    case 0:
        fadeColor = ((0) | ((0) << 5) | ((0) << 10));
        fadeOut = 0;
        break;
    case 2:
        fadeColor = (((31) | ((31) << 5) | ((31) << 10)) | 0x8000);
        fadeOut = 0;
        break;
    case 1:
        fadeColor = ((0) | ((0) << 5) | ((0) << 10));
        fadeOut = 1;
        break;
    case 3:
        fadeColor = (((31) | ((31) << 5) | ((31) << 10)) | 0x8000);
        fadeOut = 1;
        break;
    default:
        return;
    }

    switch (gWeatherPtr->currWeather)
    {
    case 3:
    case 5:
    case 13:
    case 4:
    case 6:
    case 11:
    case 12:
        useWeatherPal = 1;
        break;
    default:
        useWeatherPal = 0;
        break;
    }

    if (fadeOut)
    {
        if (useWeatherPal)
            CpuFastSet(gPlttBufferFaded, gPlttBufferUnfaded, ((0x400)/(32/8) & 0x1FFFFF));

        BeginNormalPaletteFade(0xFFFFFFFF, delay, 0, 16, fadeColor);
        gWeatherPtr->palProcessingState = 2;
    }
    else
    {
        gWeatherPtr->fadeDestColor = fadeColor;
        if (useWeatherPal)
            gWeatherPtr->fadeScreenCounter = 0;
        else
            BeginNormalPaletteFade(0xFFFFFFFF, delay, 16, 0, fadeColor);

        gWeatherPtr->palProcessingState = 1;
        gWeatherPtr->unknown_6CA = 1;
        gWeatherPtr->unknown_6CB = 0;
        Weather_SetBlendCoeffs(gWeatherPtr->currBlendEVA, gWeatherPtr->currBlendEVB);
        gWeatherPtr->readyForInit = 1;
    }
}

